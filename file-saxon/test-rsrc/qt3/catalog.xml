<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="http://www.w3.org/2010/09/qt-fots-catalog" 
         test-suite="EXPath File" version="0.1">

    <environment name="empty">
        <!-- The context item MUST be undefined/absent for queries run
             in this environment. -->
        <!-- The test query will always be an expression, and if
             necessary an empty context can be achieved by wrapping
             the expression as the body of a function -->
    </environment>
    
    <test-set name="expath-file" file="file.xml"/>  
    
</catalog>
